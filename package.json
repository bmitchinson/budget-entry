    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "sass": "^1.63.6",
    "simple-svelte-autocomplete": "^2.5.2",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.3",
